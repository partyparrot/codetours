{
  "name": "codetours",
  "private": true,
  "scripts": {
    "prestart": "test -f settings.json || touch settings.json",
    "start": "meteor run --settings settings.json",
    "lint": "eslint --quiet --ext .js,.jsx .",
    "prettier": "prettier --write \"imports/**/*.js\" \"main/**/*.js\" --single-quote=true --trailing-comma=es5 --print-width=100",
    "pretest": "npm run lint",
    "test": "jest --no-cache",
    "storybook": "start-storybook -p 9009 -s ./public",
    "build-storybook": "build-storybook -s ./public"
  },
  "dependencies": {
    "apollo-client": "^1.0.0",
    "babel-runtime": "^6.23.0",
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "graphql": "^0.9.1",
    "graphql-date": "^1.0.3",
    "graphql-server-express": "^0.6.0",
    "graphql-tag": "^2.0.0",
    "graphql-tools": "^0.11.0",
    "highlight.js": "^9.6.0",
    "lodash": "^4.15.0",
    "marked": "^0.3.6",
    "meteor-node-stubs": "~0.2.0",
    "optics-agent": "^1.1.2",
    "polished": "^1.0.2",
    "react": "^15.3.1",
    "react-apollo": "^1.0.0",
    "react-dom": "^15.3.1",
    "react-helmet": "^4.0.0",
    "react-router": "^3.0.2",
    "recompose": "^0.22.0",
    "request": "^2.74.0",
    "request-promise": "^4.1.1",
    "styled-components": "^1.4.4",
    "yaml-front-matter": "^3.4.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "apollo-test-utils": "^0.3.0",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^19.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.13.0",
    "babel-root-slash-import": "^1.1.0",
    "eslint": "^3.17.1",
    "eslint-import-resolver-meteor": "^0.3.3",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-meteor": "4.0.0",
    "eslint-plugin-no-unused-vars-rest": "^1.0.4",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^6.10.0",
    "jest": "^19.0.2",
    "jest-transform-graphql": "^1.0.0",
    "prettier": "^0.20.0",
    "storyshots": "^3.2.2"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "\\.(gql|graphql)$": "<rootDir>/.storybook/jestGraphql.js",
      "^.+\\.js$": "<rootDir>/.storybook/jestTransform.js"
    }
  }
}